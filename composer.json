{
    "name": "romanzipp/laravel-twitch",
    "description": "Twitch PHP Wrapper for Laravel",
    "license": "MIT",
    "type": "library",
    "version": "0.1",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "^5.3",
        "guzzlehttp/guzzle": "^6.2@dev"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\Twitch\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\Twitch\\Providers\\TwitchServiceProvider"
            ],
            "aliases": {
                "TwitchApi": "romanzipp\\Twitch\\Facades\\TwitchServiceFacade"
            }
        }
    }
}
